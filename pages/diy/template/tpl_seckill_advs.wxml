<view class="swiper-container adv-container" wx:if="{{ goods_error==0}}" >
    <view class="swiper-wrapper"style="width:{{ 268*adv_num }}rpx">
        <view class="swiper-slide adv-slide"  wx:for="{{ diyitem.data }}" wx:for-item="adv" >
            <navigator url="{{ adv.linkurl }}" >
                <image src="{{ adv.imgurl }}" mode="widthFix"></image>
            </navigator >
        </view>
    </view>
</view>